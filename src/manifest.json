{
  "manifest_version": 2,
  "version": "0.0.1",
  "name": "Upload to Totuslink",
  "homepage_url": "https://www.totuslink.com",
  "description": "This extension can help you upload page to Totuslink",
  "default_locale": "en",
  "permissions": [
    "activeTab",
    "cookies",
    "https://www.totuslink.com/*",
    "https://totuslink.com/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Upload to Totuslink(Alt+Shift+E)",
    "default_icon": {
      "19": "icons/16.png",
      "38": "icons/48.png"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+E",
        "mac": "Alt+Shift+E"
      },
      "description": "Toggle totuslink chrome extension controller"
    }
  }
}
